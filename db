    CREATE TABLE `php_shopping`.`users` (`id` INT(11) NOT NULL , `name` VARCHAR(255) NOT NULL , `email` VARCHAR(255) NOT NULL , `password` VARCHAR(255) NOT NULL , `address` TEXT NOT NULL , `phone` INT(11) NOT NULL , `role` TINYINT(1) NOT NULL , `created_at` DATETIME NOT NULL , `updated_at` DATETIME NOT NULL DEFAULT CURRENT_TIMESTAMP ) ENGINE = InnoDB;